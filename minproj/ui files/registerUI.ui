    selection-background-color: #45a059; /* Background color when selected */
    color: white; /* Text color */
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left-width: 0px;
    border-left-color: darkgray;
    border-left-style: solid; /* creates triangle */
    border-top-right-radius: 5px; /* Adjust as needed for curved corner */
    border-bottom-right-radius: 5px; /* Adjust as needed for curved corner */
    margin-right: 5px; /* Move arrow a little bit to the left */
}

QComboBox::down-arrow {
    image:url(:/images/down-arrow.png); /* Custom arrow icon */
    height:25px;
    width: 30px;
    margin-right: 7px;
    margin-top: 7px;
}

QComboBox::down-arrow:on {
    /* Rotate arrow when dropdown is opened */
    transform: rotate(180deg);
}

QComboBox::item {
    height: 25px;
}

QComboBox::item:selected {
    background-color: #45a059;
    color: white;
}
